{
  "cli": {
    "version": ">= 9.1.0",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "preview"
    },
    "debug": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "channel": "debug"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "alpha"
      },
      "ios": {
        "appleId": "enmanuelmag@gmail.com",
        "appName": "<replace-with-your-app-name>",
        "ascAppId": "<replace-with-your-app-id>"
      }
    },
    "closed": {
      "android": {
        "track": "alpha"
      },
      "ios": {
        "appleId": "enmanuelmag@gmail.com",
        "appName": "<replace-with-your-app-name>",
        "ascAppId": "<replace-with-your-app-id>"
      }
    },
    "internal": {
      "android": {
        "track": "beta"
      },
      "ios": {
        "appleId": "enmanuelmag@gmail.com",
        "appName": "<replace-with-your-app-name>",
        "ascAppId": "<replace-with-your-app-id>"
      }
    }
  }
}